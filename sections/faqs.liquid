{% capture children %}
  {% if section.settings.faqs_filter %}
    <div class="faqs-filterDropdown-container flex items-baseline flex-wrap gap-2xl">
      {% if section.settings.faqs_filter_title != blank %}
        <span class="text">{{ section.settings.faqs_filter_title }}</span>
      {% endif %}
      <details id="Details-FAQsFilter" class="details--dropdown faqs-filterDropdown relative" is="dropdown-details" activate-event="hover" data-index="0">
        <summary
          id="FAQsFilter"
          class="details__summary faqs-filterDropdown-summary link link--text focus-inset caption-large"
          data-faqs-filter
        >
          <span class="flex items-center justify-between w-full gap-xl">
            <span class="text">All</span>
            {{- 'icon-caret.svg' | inline_asset_content -}}
          </span>
        </summary>
        <div
          id="FAQsFilterDropdown"
          class="details__list absolute top-full left-0 z-1 global-settings-popup"
          style="--min-w-md: 26.7rem;"
          tabindex="-1"
        >
          <ul
            class="list-unstyled faqs-filterDropdown-menu"
            role="list"
            style="--p: 1.3rem 2rem;"
          >
            <li class="active" tabindex="-1"><span class="text">All</span></li>
          </ul>
        </div>
      </details>
    </div>
  {% endif %}

  {% content_for 'blocks' %}
{% endcapture %}


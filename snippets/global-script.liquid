<script src="{{ 'vendor.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'global.js' | asset_url }}" defer="defer"></script>

{%- if settings.show_calculator_free_shipping_message -%}
  <script src="{{ 'currencies.js' | shopify_asset_url }}" defer="defer"></script>
  <script src="{{ 'free-shipping-message.js' | asset_url }}" defer="defer"></script>
{%- endif %}

{%- if settings.show_cart_quick_edit_plus -%}
  <script src="{{ 'edit-cart.js' | asset_url }}" defer="defer"></script>
{%- endif %}

{%- if settings.show_countdown -%}
  <script src="{{ 'countdown.js' | asset_url }}" defer="defer"></script>
{%- endif %}

{%- if localization.available_countries.size > 1 or localization.available_languages.size > 1 -%}
  <script src="{{ 'localization-form.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

{%- if settings.animations_reveal_on_scroll -%}
  <script src="{{ 'animations.js' | asset_url }}" defer="defer"></script>
{%- endif -%}
<script src="{{ 'text-animation.js' | asset_url }}" defer="defer"></script>

<script>
  window.shopUrl = '{{ request.origin }}';
  window.routes = {
    shop_origin: '{{ request.origin }}',
    root: '{{  routes.cart_url | split: "/cart" | first }}',
    root_url: '{{ routes.root_url }}',
    cart_add_url: '{{ routes.cart_add_url }}',
    cart_change_url: '{{ routes.cart_change_url }}',
    cart_update_url: '{{ routes.cart_update_url }}',
    cart_url: '{{ routes.cart_url }}',
    collection_all: '{{ routes.all_products_collection_url }}',
    predictive_search_url: '{{ routes.predictive_search_url }}',
    search_url: '{{ routes.search_url }}'
  };

  window.inventory_text = {
    warningQuantity: `{{ 'products.product.quantity.warning_quantity' | t : inventory: '[inventory]' }}`
  };

  window.button_load_more = {
    default: `{{ 'content.show_more' | t }}`,
    loading: `{{ 'content.loading' | t }}`,
    view_all: `{{ 'content.view_all' | t }}`,
    no_more: `{{ 'content.no_more' | t }}`,
    no_more_collection: `{{ 'content.no_more_collection' | t }}`
  };

  window.cartStrings = {
    error: `{{ 'content.cart_error' | t }}`,
    quantityError: `{{ 'content.cart_quantity_error_html' | t: quantity: '[quantity]' }}`,
    item: `{{ 'content.cart_count.one' | t: count: '[count]' }}`,
    items: `{{ 'content.cart_count.other' | t: count: '[count]' }}`,
    item_99: `{{ 'content.cart_count_max.more' | t }}`
  };

  window.variantStrings = {
    addToCart: `{{ 'products.product.add_to_cart' | t }}`,
    addingToCart: `{{ 'products.product.adding_to_cart' | t }}`,
    chooseOptions: `{{ 'products.product.choose_options' | t }}`,
    soldOut: `{{ 'products.product.sold_out' | t }}`,
    preOrder: `{{ 'products.product.pre_order' | t }}`,
    unavailable: `{{ 'products.product.unavailable' | t }}`,
    unavailable_with_option: `{{ 'products.product.value_unavailable' | t: option_value: '[value]' }}`
  };

  window.quickOrderListStrings = {
    itemsAdded: `{{ 'content.items_added.other' | t: quantity: '[quantity]' }}`,
    itemAdded: `{{ 'content.items_added.one' | t: quantity: '[quantity]' }}`,
    itemsRemoved: `{{ 'content.items_removed.other' | t: quantity: '[quantity]' }}`,
    itemRemoved: `{{ 'content.items_removed.one' | t: quantity: '[quantity]' }}`,
    viewCart: `{{- 'content.view_cart' | t -}}`,
    each: `{{- 'content.each' | t: money: '[money]' }}`,
    min_error: `{{- 'content.min_error' | t: min: '[min]' }}`,
    max_error: `{{- 'content.max_error' | t: max: '[max]' }}`,
    step_error: `{{- 'content.step_error' | t: step: '[step]' }}`
  };

  window.accessibilityStrings = {
    imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
    shareSuccess: `{{ 'content.share.success_message' | t }}`,
    pauseSlideshow: `{{ 'content.pause_slideshow' | t }}`,
    playSlideshow: `{{ 'content.play_slideshow' | t }}`,
    recipientFormExpanded: `{{ 'gift_cards.expanded' | t }}`,
    recipientFormCollapsed: `{{ 'gift_cards.collapsed' | t }}`,
    countrySelectorSearchCount: `{{ 'content.country_results_count' | t: count: '[count]' }}`
  };

  window.shippingCalculatorStrings = {
    error: {{ 'content.calculator_shipping.error' | t | json }},
    not_found: {{ 'content.calculator_shipping.not_found' | t | json }},
    one_result: {{ 'content.calculator_shipping.one_result' | t | json }},
    multiple_results: {{ 'content.calculator_shipping.multiple_results' | t | json }}
  };

  window.countdown = {
    day: `{{ 'content.day' | t }}`,
    hour: `{{ 'content.hour' | t }}`,
    min: `{{ 'content.min' | t }}`,
    sec: `{{ 'content.sec' | t }}`,
    days: `{{ 'content.days' | t }}`,
    hours: `{{ 'content.hours' | t }}`,
    mins: `{{ 'content.mins' | t }}`,
    secs: `{{ 'content.secs' | t }}`,
    d: `{{ 'content.d' | t }}`,
    h: `{{ 'content.h' | t }}`,
    m: `{{ 'content.m' | t }}`,
    s: `{{ 'content.s' | t }}`
  };

  window.dynamicBrowserTitle = {
    show: {{ settings.show_dynamic_browser_title }},
    text: '{{ settings.dynamic_browser_title_content | replace: "'","\'" }}'
  };

  window.floatingIcons = {
    show: {{ settings.show_floating_icons }},
    icons: '{{ settings.floating_icons }}',
    speed: {{ settings.floating_icons_speed | default: 100 }}
  };

  {% if settings.show_calculator_free_shipping_message %}
    {% if settings.calculator_free_shipping_message_price != blank %}
      window.free_shipping_price = {{ settings.calculator_free_shipping_message_price }};
    {% else %}
      window.free_shipping_price = 0;
    {% endif %}
    window.free_shipping_default_currency = '{{ shop.currency }}';
    window.free_shipping_text = {
      free_shipping_message: `{{ 'content.free_shipping_message' | t }}`,
      free_shipping_message_1: `{{ 'content.free_shipping_message_1' | t }}`,
      free_shipping_message_2:`{{ 'content.free_shipping_message_2' | t }}`,
      free_shipping_message_3: `{{ 'content.free_shipping_message_3' | t }}`,
      free_shipping_message_4: `{{ 'content.free_shipping_message_4' | t }}`,
      free_shipping_1: `{{ 'content.free_shipping_1' | t }}`,
      free_shipping_2: `{{ 'content.free_shipping_2' | t }}`
    };
  {% endif %}

  window.wishlist = {
    add: `{{ 'content.add' | t }}`,
    added: `{{ 'content.added' | t }}`,
    empty: `{{ 'content.no_product_added' | t }}`,
    continue_shopping: `{{ 'content.continue_shopping' | t }}`,
    remove: `{{ 'content.remove' | t }}`
  };

  window.compare = {
    show: {{ settings.enable_compare }},
    add: `{{ 'content.compare.add' | t }}`,
    added: `{{ 'content.compare.added' | t }}`,
    message: `{{ 'content.compare.message' | t }}`
  };

  {% if settings.currency_format_enable %}
    window.money_format = '{{ shop.money_with_currency_format }}';
  {% else %}
    window.money_format = '{{ shop.money_format }}';
  {% endif %}

  window.info = {
    name: 'ELLA Theme',
    version: 'v7.0.3 - 2025/12/11'
  };

  document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
</script>